<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

	<extension point="net.sourceforge.plantuml.eclipse.tests.testProject">
		<testProject
			projectName="textfiles"
			/>		
	</extension>
	<extension point="net.sourceforge.plantuml.eclipse.tests.testFile">
		<testFile
			sourceFileUri="platform:/plugin/net.sourceforge.plantuml.text.tests/testFiles/diagram.md"
			targetPath="/textfiles"
			/>
		<testFile
			sourceFileUri="platform:/plugin/net.sourceforge.plantuml.text.tests/testFiles/diagram.adoc"
			targetPath="/textfiles"
			/>
		<testFile
			sourceFileUri="platform:/plugin/net.sourceforge.plantuml.text.tests/testFiles/multi.plantuml"
			targetPath="/textfiles"
			selectionsProvider="net.sourceforge.plantuml.text.test.TextEditorSelectionIterable"
			/>
	</extension>
</plugin>
