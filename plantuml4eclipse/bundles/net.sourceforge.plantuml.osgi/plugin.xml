<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>

   <extension
         point="net.sourceforge.plantuml.eclipse.diagramIntentProvider">

      <diagramIntentProvider
      	id="net.sourceforge.plantuml.osgi.ManifestDiagramIntentProvider"
      	providerClass="net.sourceforge.plantuml.osgi.ManifestDiagramIntentProvider"
      	label="Bundle dependency diagrams generated from OSGi manifests"
      	>
      </diagramIntentProvider>
      <diagramIntentProvider
        id="net.sourceforge.plantuml.osgi.OsgiComponentDiagramIntentProvider"
        providerClass="net.sourceforge.plantuml.osgi.OsgiComponentDiagramIntentProvider"
      	label="Component diagrams generated from OSGi manifests and component XML files"
        >
      </diagramIntentProvider>
   </extension>

</plugin>
